    containers,
    network-uri,
    monad-loops,
    wreq,
    lens,
    bytestring,
    parsec,
    utf8-string,
    tagsoup,
    xml,
    feed,
    regex-pcre,
    aeson,
    network,
    wai,
    wai-extra,
    wai-cors,
    warp,
    servant-server >= 0.4,
    rethinkdb,
    transformers,
    either,
    unordered-containers,
    mtl,
    http-types,
    safe,
    hashable,
    resource-pool,
    time,
    pooled-io,
    shelly,
    string-conversions,
    bcrypt,
    random,
    jwt

